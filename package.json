  "author": "Eric Schmidt",
  "license": "ISC",
  "private": false,
  "dependencies": {
    "@contentful/app-sdk": "4.12.1",
    "@contentful/f36-components": "4.20.6",
    "@contentful/f36-tokens": "4.0.1",
    "@contentful/field-editor-json": "^3.1.0",
    "@contentful/react-apps-toolkit": "1.2.7",
    "contentful-management": "10.14.0",
    "emotion": "10.0.27",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "dotenv cross-var contentful-app-scripts upload --ci --bundle-dir ./build  --organization-id %ORGANIZATION_ID% --definition-id %APP_DEFINITION_ID% --token %ACCESS_TOKEN%"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.1.23",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.3.0"
  },
  "homepage": "."
}
